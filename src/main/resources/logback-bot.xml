<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %level [%thread] %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<root level="error">
		<appender-ref ref="STDOUT"/>
	</root>
	
	<logger name="conflux.dex" level="info"/>
</configuration>